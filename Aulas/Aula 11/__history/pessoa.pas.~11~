unit pessoa;

interface

type
  TPessoa = class
    private
    FNome: String;
    FSobrenome: String;
    public
      property Nome: String read FNome write FNome;
      property Sobrenome: String read FSobrenome write FSobrenome;
  end;

type
  TCliente = class(TPessoa)
    private
    public
      function NomeCompleto : String;
  end;

implementation

end.
