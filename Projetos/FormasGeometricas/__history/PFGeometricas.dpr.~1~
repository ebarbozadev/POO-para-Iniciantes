program PFGeometricas;

uses
  Vcl.Forms,
  frmPrincipal in 'frmPrincipal.pas' {Formas_Geométricas},
  frmPerimetro in 'frmPerimetro.pas' {Perímetro};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFormas_Geométricas, Formas_Geométricas);
  Application.CreateForm(TPerimetro, Perimetro);
  Application.Run;
end.
