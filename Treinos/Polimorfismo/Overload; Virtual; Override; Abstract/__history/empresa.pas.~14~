unit empresa;

interface

uses contabancaria;

type
  TEmpresa = class(TContaBancaria)
    private
    FRazaoSocial: String;
    public
      property RazaoSocial  : String read FRazaoSocial write FRazaoSocial;

      function RetornaDados        : String; override;
  end;

implementation

{ TEmpresa }

function TEmpresa.RetornaDados: String;
begin
  inherited;
  Dados:= Dados + ' Razão Social: ' + FRazaoSocial;
  Result:= Dados;
end;

end.
